package cn.plou.web.system.meterMessage.mbus.vo;

import org.springframework.web.bind.annotation.RequestParam;

import java.util.List;

public class MbusControls {
    List<String> mbusCodes;
    String consumerId;
    String mbusControl;
    String mbusControlParam;

    public List<String> getMbusCodes() {
        return mbusCodes;
    }

    public void setMbusCodes(List<String> mbusCodes) {
        this.mbusCodes = mbusCodes;
    }

    public String getConsumerId() {
        return consumerId;
    }

    public void setConsumerId(String consumerId) {
        this.consumerId = consumerId;
    }

    public String getMbusControl() {
        return mbusControl;
    }

    public void setMbusControl(String mbusControl) {
        this.mbusControl = mbusControl;
    }

    public String getMbusControlParam() {
        return mbusControlParam;
    }

    public void setMbusControlParam(String mbusControlParam) {
        this.mbusControlParam = mbusControlParam;
    }
}
