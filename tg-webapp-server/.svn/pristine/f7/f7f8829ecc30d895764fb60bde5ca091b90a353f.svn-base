package cn.plou.web.charge.chargeconfig.vo;

import java.math.BigDecimal;

public class HomePageTopRightVO {

    private String chargeStandard;
    private BigDecimal sumAccount;
    private BigDecimal sumHeatingValue;


    public String getChargeStandard() {
        return chargeStandard;
    }

    public void setChargeStandard(String chargeStandard) {
        this.chargeStandard = chargeStandard;
    }

    public BigDecimal getSumAccount() {
        return sumAccount;
    }

    public void setSumAccount(BigDecimal sumAccount) {
        this.sumAccount = sumAccount;
    }

    public BigDecimal getSumHeatingValue() {
        return sumHeatingValue;
    }

    public void setSumHeatingValue(BigDecimal sumHeatingValue) {
        this.sumHeatingValue = sumHeatingValue;
    }

}
