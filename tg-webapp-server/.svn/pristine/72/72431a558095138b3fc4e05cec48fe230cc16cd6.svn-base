    private BigDecimal maxTemperatureDeviation;
    private BigDecimal maxPressureDeviation;
    private BigDecimal maxHeatingIndex;
    private BigDecimal maxFlowingIndex;
    private BigDecimal minFlowingDeviation;
    private BigDecimal minTemperatureDeviation;
    private BigDecimal minPressureDeviation;
    private BigDecimal minHeatingIndex;
    private BigDecimal minFlowingIndex;

    public BigDecimal getMaxFlowingDeviation() {
        return maxFlowingDeviation;
    }

    public void setMaxFlowingDeviation(BigDecimal maxFlowingDeviation) {
        this.maxFlowingDeviation = maxFlowingDeviation;
    }

    public BigDecimal getMaxTemperatureDeviation() {
        return maxTemperatureDeviation;
    }

    public void setMaxTemperatureDeviation(BigDecimal maxTemperatureDeviation) {
        this.maxTemperatureDeviation = maxTemperatureDeviation;
    }

    public BigDecimal getMaxPressureDeviation() {
        return maxPressureDeviation;
    }

    public void setMaxPressureDeviation(BigDecimal maxPressureDeviation) {
        this.maxPressureDeviation = maxPressureDeviation;
    }


    public BigDecimal getMinFlowingDeviation() {
        return minFlowingDeviation;
    }

    public void setMinFlowingDeviation(BigDecimal minFlowingDeviation) {
        this.minFlowingDeviation = minFlowingDeviation;
    }

    public BigDecimal getMinTemperatureDeviation() {
        return minTemperatureDeviation;
    }

    public void setMinTemperatureDeviation(BigDecimal minTemperatureDeviation) {
        this.minTemperatureDeviation = minTemperatureDeviation;
    }

    public BigDecimal getMinPressureDeviation() {
        return minPressureDeviation;
    }

    public void setMinPressureDeviation(BigDecimal minPressureDeviation) {
        this.minPressureDeviation = minPressureDeviation;
    }

    public BigDecimal getMaxHeatingIndex() {
        return maxHeatingIndex;
    }

    public void setMaxHeatingIndex(BigDecimal maxHeatingIndex) {
        this.maxHeatingIndex = maxHeatingIndex;
    }

    public BigDecimal getMaxFlowingIndex() {
        return maxFlowingIndex;
    }

    public void setMaxFlowingIndex(BigDecimal maxFlowingIndex) {
        this.maxFlowingIndex = maxFlowingIndex;
    }

    public BigDecimal getMinHeatingIndex() {
        return minHeatingIndex;
    }

    public void setMinHeatingIndex(BigDecimal minHeatingIndex) {
        this.minHeatingIndex = minHeatingIndex;
    }

    public BigDecimal getMinFlowingIndex() {
        return minFlowingIndex;
    }

    public void setMinFlowingIndex(BigDecimal minFlowingIndex) {
        this.minFlowingIndex = minFlowingIndex;
    }
