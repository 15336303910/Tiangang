package cn.plou.web.heatManage.dataAnalysis.domain;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 * 
 * @author admin
 *
 */
public class HouseRunningDataAnalysisTotalGatherDO implements Serializable {
	private static final long serialVersionUID = 1L;


	//显示的标题
	private String paramTitle;
	//'用户数量
	private Long consumerCount;
	//实际耗热指标
	private BigDecimal heatingIndex;
	//实际流量指标
	private BigDecimal flowingIndex;


	//平均进水温度
	private BigDecimal inTemperature;
	//平均回水温度
	private BigDecimal outTemperature;
	//平均室温
	private BigDecimal roomTemperatureRead;


	//平均温差
	private BigDecimal roomTemperatureDifference;


	//供暖面积
	private BigDecimal heatingArea;

	/**
	 * 设置：进水温度
	 */
	public void setInTemperature(BigDecimal inTemperature) {
		this.inTemperature = inTemperature;
	}
	/**
	 * 获取：进水温度
	 */
	public BigDecimal getInTemperature() {
		return inTemperature;
	}
	/**
	 * 设置：回水温度
	 */
	public void setOutTemperature(BigDecimal outTemperature) {
		this.outTemperature = outTemperature;
	}
	/**
	 * 获取：回水温度
	 */
	public BigDecimal getOutTemperature() {
		return outTemperature;
	}
	/**
	 * 设置：室温测量值
	 */
	public void setRoomTemperatureRead(BigDecimal roomTemperatureRead) {
		this.roomTemperatureRead = roomTemperatureRead;
	}
	/**
	 * 获取：室温测量值
	 */
	public BigDecimal getRoomTemperatureRead() {
		return roomTemperatureRead;
	}

	/**
	 * 设置：供暖面积
	 */
	public void setHeatingArea(BigDecimal heatingArea) {
		this.heatingArea = heatingArea;
	}
	/**
	 * 获取：供暖面积
	 */
	public BigDecimal getHeatingArea() {
		return heatingArea;
	}
	/**
	 * 设置：耗热指标
	 */
	public void setHeatingIndex(BigDecimal heatingIndex) {
		this.heatingIndex = heatingIndex;
	}
	/**
	 * 获取：耗热指标
	 */
	public BigDecimal getHeatingIndex() {
		return heatingIndex;
	}
	/**
	 * 设置：流量指标
	 */
	public void setFlowingIndex(BigDecimal flowingIndex) {
		this.flowingIndex = flowingIndex;
	}
	/**
	 * 获取：流量指标
	 */
	public BigDecimal getFlowingIndex() {
		return flowingIndex;
	}


	public Long getConsumerCount() {
		return consumerCount;
	}

	public void setConsumerCount(Long consumerCount) {
		this.consumerCount = consumerCount;
	}

	public BigDecimal getRoomTemperatureDifference() {
		return roomTemperatureDifference;
	}

	public void setRoomTemperatureDifference(BigDecimal roomTemperatureDifference) {
		this.roomTemperatureDifference = roomTemperatureDifference;
	}


	public String getParamTitle() {
		return paramTitle;
	}

	public void setParamTitle(String paramTitle) {
		this.paramTitle = paramTitle;
	}
}
